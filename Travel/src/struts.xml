<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<constant name="struts.devMode" value="true" />

	<package name="com.internousdev.travel.action" extends="struts-default">

		<action name="HomeAction" class="com.internousdev.travel.action.HomeAction"
			method="execute">
			<result name="success">index.jsp</result>
		</action>

		<action name="LoginAction" class="com.internousdev.travel.action.LoginAction"
			method="execute">
			<result name="success">login.jsp</result>
		</action>

		<action name="LoginConfirmAction"
			class="com.internousdev.travel.action.LoginConfirmAction" method="execute">
			<result name="success">loginConfirm.jsp</result>
			<result name="error">loginError.jsp</result>
		</action>

		<action name="UserCreateAction" class="com.internousdev.travel.action.UserCreateAction"
			method="execute">
			<result name="success">userCreate.jsp</result>
		</action>

		<action name="UserCreateConfirmAction"
			class="com.internousdev.travel.action.UserCreateConfirmAction"
			method="execute">
			<result name="success">userCreateConfirm.jsp</result>
			<result name="error">userCreate.jsp</result>
		</action>

		<action name="UserCreateCompleteAction"
			class="com.internousdev.travel.action.UserCreateCompleteAction"
			method="execute">
			<result name="success">userCreateComplete.jsp</result>
		</action>

		<action name="BuyItemAction" class="com.internousdev.travel.action.BuyItemAction" method="execute">
		<result name="success">buyItem.jsp</result>
		</action>

		<action name="BuyItemConfirmAction" class="com.internousdev.travel.action.BuyItemConfirmAction" method="execute">
		<result name="success">buyItemConfirm.jsp</result>

		</action>

		<action name="BuyItemCompleteAction" class="com.internousdev.travel.action.BuyItemCompleteAction" method="execute">
		<result name="success">buyItemComplete.jsp</result>

		</action>

		<action name="MyPageAction" class="com.internousdev.travel.action.MyPageAction" method="execute">
		<result name="error">myPage.jsp</result>

		</action>

		<action name="CountryAction" class="com.internousdev.travel.action.CountryAction" method="execute">
		<result name="success">counrty.jsp</result>

		</action>

		<action name="JapanAction" class="com.internousdev.travel.action.JapanAction" method="execute">
		<result name="success">japan.jsp</result>
		</action>

		<action name="KoreaAction" class="com.internousdev.travel.action.KoreaAction" method="execute">
		<result name="success">korea.jsp</result>
		</action>

		<action name="AusAction" class="com.internousdev.travel.action.AusAction" method="execute">
		<result name="success">aus.jsp</result>
		</action>

		<action name="UsaAction" class="com.internousdev.travel.action.UsaAction" method="execute">
		<result name="success">usa.jsp</result>
		</action>

		<action name="CanadaAction" class="com.internousdev.travel.action.CanadaAction" method="execute">
		<result name="success">canada.jsp</result>
		</action>











	</package>





</struts>